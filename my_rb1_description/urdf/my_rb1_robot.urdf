<?xml version="1.0" ?>
<robot name="rb1_robot">

    <!-- Define materials for visualization -->
    <material name="blue">
        <color rgba="0 0 1 1"/>
    </material>

    <material name="gray">
        <color rgba="0.40 0.40 0.40 1"/>
    </material>

    <material name="red">
        <color rgba="1 0 0 1"/>
    </material>

    <!-- Base footprint link -->
    <link name="base_footprint">
    </link>

    <!-- Joint connecting base_link and base_footprint -->
    <!-- TO DO: Set correct joint type -->
    <joint name="base_link_plate_joint" type="fixed">
        <!-- TO DO: Define axis (if needed) -->
        <axis xyz="0 0 0"/>
        <origin xyz="0 0 0" rpy = "0 0 0"/>
        <parent link="base_link"/>
        <child link="base_footprint"/>
    </joint>

    <!-- Base link -->
    <link name="base_link">
        
        <visual>
            <geometry>
                <cylinder radius ="0.25" length ="0.30"/>
            </geometry>
            <material name="blue"/>       
        </visual>
        <collision>
        <geometry>
            <cylinder radius =".25" length =".30"/>
        </geometry>
        </collision>   
        <inertial>
            <mass value="16" />
            <origin xyz="0 0 0.3" rpy="0 0 0" />
            <inertia ixx="0.65" ixy="0" ixz="0" iyy="0.65" iyz="0" izz="0.5" />
        </inertial>

    </link>

    <!-- Left wheel link -->
    <link name="left_wheel">            
        <visual>
        
        <geometry>
            <cylinder radius =".025" length =".020"/>
        </geometry>
        <material name="red"/>       
        </visual>
        <collision>
        
        <geometry>
            <cylinder radius =".025" length =".020"/>
        </geometry>
        </collision>   
        <inertial>
            <mass value="2" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00364" ixy="0" ixz="0" iyy="0.00364" iyz="0" izz="0.000625" />
        </inertial>
    </link>

    <!-- Joint connecting base_link and left_wheel -->
    <!-- TO DO: Set correct joint type --> <!--0.3005 0 -0.1-->
    <joint name="base_link_left_wheel_joint" type="continuous">
        <origin xyz="0 -0.2 -0.15" rpy="1.57 0 0 " />
        <child link="left_wheel" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 0 1" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1" />
    </joint>

    <!-- Right wheel link -->
    <link name="right_wheel">
         <visual>
        <geometry>
            <cylinder radius =".025" length =".020"/>
        </geometry>
        <material name="red"/>       
        </visual>
        <collision>
        <geometry>
            <cylinder radius =".025" length =".020"/>
        </geometry>
        </collision>   
        <inertial>
            <mass value="2" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00364" ixy="0" ixz="0" iyy="0.00364" iyz="0" izz="0.000625" />
        </inertial>
    </link>

    <!-- Joint connecting base_link and right_wheel -->
    <!-- TO DO: Set correct joint type -->
    <joint name="base_link_right_wheel_joint" type="continuous">
        <origin xyz="0 0.2 -0.15" rpy="1.57 0 0 " />
        <child link="right_wheel" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 0 1" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>

    <!-- Front caster -->
    <link name="front_caster">
        <visual>
        <geometry>
            <!--<cylinder radius =".025" length =".010"/>-->
            <sphere radius = "0.025"/>
        </geometry>
        <material name="gray"/>       
        </visual>
        <collision>
        <geometry>
            <!--<cylinder radius =".025" length =".010"/>-->
            <sphere radius = "0.025"/>
        </geometry>
        </collision>   
        <inertial>
            <mass value="1.5" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <!--<inertia ixx="0.001485" ixy="0" ixz="0" iyy="0.001485" iyz="0" izz="0.00046875" />-->
            <inertia ixx="0.000375" ixy="0" ixz="0" iyy="0.000375" iyz="0" izz="0.000375" />
        </inertial>
    </link>


    <!-- Joint connecting base_link and front_caster -->

    <joint name="base_link_front_caster_joint" type="continuous">
        <origin xyz="-0.1 0 -0.15" rpy="1.57 0 0 " />
        <child link="front_caster" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 1 1" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1" friction="1" />
    </joint>


    <!-- Back caster -->
    <link name="back_caster">
        <visual>
        <geometry>
            <!--<cylinder radius =".025" length =".010"/>-->
            <sphere radius = "0.025"/>
        </geometry>
        <material name="gray"/>       
        </visual>
        <collision>
        <geometry>
            <!--<cylinder radius =".025" length =".010"/>-->
            <sphere radius = "0.025"/>
        </geometry>
        </collision>   
        <inertial>
            <mass value="1.5" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <!--<inertia ixx="0.001485" ixy="0" ixz="0" iyy="0.001485" iyz="0" izz="0.00046875" />-->
            <inertia ixx="0.000375" ixy="0" ixz="0" iyy="0.000375" iyz="0" izz="0.000375" />
        </inertial>
    </link>


    <!-- Joint connecting base_link and front_caster -->

    <joint name="base_link_back_caster_joint" type="continuous">
        <origin xyz="0.1 0 -0.15" rpy="0 0 0 " />
        <child link="back_caster" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 1 1" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1" friction="1" />
    </joint>

    <!-- Link representing the front laser scanner -->
    <!--For this robot, the laser is not visible, so we won't define a body for it. -->
    <link name="front_laser">
        <inertial>
            <mass value="2" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00198" ixy="0" ixz="0" iyy="0.00198" iyz="0" izz="0.000625" />
        </inertial>
    </link>

    <!-- Joint connecting base_link and front_laser -->
    <!-- TO DO: Set correct joint type -->
    <joint name="base_link_front_laser_joint" type="fixed">
        <origin xyz="0.25 0 0.075" rpy="3.14 0 0 " />
        <child link="front_laser" />
        <parent link="base_link" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>



<!-- Gazebo visualization -->
<gazebo reference="base_link">
    <material>Gazebo/Black</material>
</gazebo>

<gazebo reference="left_wheel">
    <!-- TODO: Define mu1 (first friction coefficient) -->
    <!-- TODO: Define mu2 (second friction coefficient) -->
    <!-- TODO: Define kp (contact stiffness) -->
    <!-- TODO: Define kd (contact damping) -->
    <!-- TODO: Define fdir1 (friction direction) -->
    <!-- TODO: Choose a material for visualization -->
    <mu1>0.5</mu1>
    <mu2>0.2</mu2>
    <kp>1000</kp>
    <kd>2</kd>
    
    <fdir1>1 0 0</fdir1>
    <material>Gazebo/Orange</material>


</gazebo>

<gazebo reference="right_wheel">
    <!-- TODO: Define mu1 (first friction coefficient) -->
    <!-- TODO: Define mu2 (second friction coefficient) -->
    <!-- TODO: Define kp (contact stiffness) -->
    <!-- TODO: Define kd (contact damping) -->
    <!-- TODO: Define fdir1 (friction direction) -->
    <!-- TODO: Choose a material for visualization -->
    <mu1>0.5</mu1>
    <mu2>0.2</mu2>
    <kp>1000</kp>
    <kd>2</kd>
    
    <fdir1>1 0 0</fdir1>
    <material>Gazebo/Orange</material>
</gazebo>

<gazebo reference="front_caster">
    <mu1>0</mu1>
    <mu2>0</mu2>
    <kp>1000</kp>
    <kd>2</kd>
    
    <fdir1>1 0 0</fdir1>
    <material>Gazebo/Red</material>
</gazebo>

<gazebo reference="back_caster">
    <mu1>0</mu1>
    <mu2>0</mu2>
    <kp>1000</kp>
    <kd>2</kd>

    <fdir1>1 0 0</fdir1>
    <material>Gazebo/Red</material>
    
</gazebo>
<!--
<gazebo reference="base_link_front_caster_joint">
    <preserveFixedJoint>true</preserveFixedJoint>
</gazebo>

<gazebo reference="base_link_back_caster_joint">
    <preserveFixedJoint>true</preserveFixedJoint>
</gazebo>
-->

<gazebo reference="base_link_front_laser_joint">
    <preserveFixedJoint>true</preserveFixedJoint>
</gazebo>

    

<!--Gazebo Plugins-->
<gazebo>
    <plugin filename="libgazebo_ros_diff_drive.so" name="differential_drive_controller">
        <alwaysOn>true</alwaysOn>
        <updateRate>20</updateRate>
        <leftJoint>base_link_left_wheel_joint</leftJoint>
        <rightJoint>base_link_right_wheel_joint</rightJoint>
        <wheelSeparation>0.41</wheelSeparation>
        <wheelDiameter>0.05</wheelDiameter>
        <torque>20</torque>
        <commandTopic>cmd_vel</commandTopic>
        <odometryTopic>odom</odometryTopic>
        <odometryFrame>odom</odometryFrame>
        <robotBaseFrame>base_link</robotBaseFrame>
        
    </plugin>
</gazebo>

<gazebo reference="front_laser">
    <sensor type="ray" name="head_hokuyo_sensor">
        <pose>0 0 0 0 0 0</pose>
        <visualize>false</visualize>
        <update_rate>20</update_rate>
        <ray>
            <scan>
                <horizontal>
                    <samples>720</samples>
                    <resolution>1</resolution>
                    <min_angle>-1.570796</min_angle>
                    <max_angle>1.570796</max_angle>
                </horizontal>
            </scan>
            <range>
                <min>0.20</min>
                <max>10.0</max>
                <resolution>0.01</resolution>
            </range>
            <noise>
                <type>gaussian</type>
                <mean>0.0</mean>
                <stddev>0.01</stddev>
            </noise>
        </ray>
        <plugin name="gazebo_ros_head_hokuyo_controller" filename="libgazebo_ros_laser.so">
            <topicName>/scan</topicName>
            <frameName>sensor_laser</frameName>
        </plugin>
    </sensor>
</gazebo>


    </robot>